<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Spinner
        android:id="@+id/spinnerBrand"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/viewMain" />
    <Spinner
        android:id="@+id/spinnerModel"
        android:layout_width="250sp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/spinnerBrand"
        android:layout_below="@+id/spinnerBrand"
        android:layout_toLeftOf="@+id/viewMain" />
    <Spinner
        android:id="@+id/spinnerYear"
        android:layout_width="250sp"
        android:layout_height="60dp"
        android:layout_below="@+id/spinnerModel"
        android:layout_toLeftOf="@+id/viewMain" />
    <View
        android:id="@+id/viewMain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/spinnerBrand"
        android:background="@color/Gray" />
    <RelativeLayout
        android:id="@+id/canvas_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/txtFinished"
        android:layout_alignLeft="@+id/viewMain"
        android:layout_alignRight="@+id/viewMain"
        android:layout_alignTop="@+id/viewMain"
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginTop="2dp"
        android:background="@color/blue" >
        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txx1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/insurcap"
                    android:textSize="35sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txx2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/insur"
                    android:textSize="35sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txx3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/insurtotal"
                    android:textSize="35sp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/linearLayout1" >

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <EditText
                    android:id="@+id/txtInput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:textSize="30sp"
                    android:ems="30"
                    android:inputType="number" >
                </EditText>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txtInsur"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/o"
                    android:textSize="35sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txtInsurTotal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/o"
                    android:textSize="35sp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/linearLayout2" >

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txx4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/repairc"
                    android:textSize="35sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txtRepairC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/o"
                    android:textSize="35sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="2.5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txtRepairCTotal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/o"
                    android:textSize="35sp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/linearLayout3" >

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <Button
                    android:id="@+id/btnCal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:text="@string/cal"
                    android:textSize="25sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txtMin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/o"
                    android:textSize="30sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txtAvg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/o"
                    android:textSize="30sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="50dp"
                android:layout_height="92dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="2.5dp"
                android:layout_weight="1"
                android:background="@color/Gray" >

                <TextView
                    android:id="@+id/txtMax"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/o"
                    android:textSize="30sp" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/spinnerYear"
        android:layout_below="@+id/spinnerYear"
        android:layout_marginTop="20dp"
        android:text="@string/carage"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv4"
        android:layout_below="@+id/tv4"
        android:layout_marginTop="20dp"
        android:text="@string/machine"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv5"
        android:layout_below="@+id/tv5"
        android:layout_marginTop="20dp"
        android:text="@string/cargroup"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv6"
        android:layout_below="@+id/tv6"
        android:layout_marginTop="20dp"
        android:text="@string/protection"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv7"
        android:layout_below="@+id/tv7"
        android:layout_marginTop="20dp"
        android:text="@string/finished"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtMachine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv6"
        android:layout_alignRight="@+id/txtCarAge"
        android:text="@string/o"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtCarGroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv7"
        android:layout_alignRight="@+id/txtMachine"
        android:text="@string/o"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtProtection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv8"
        android:layout_alignRight="@+id/txtCarGroup"
        android:text="@string/o"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtFinished"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tv8"
        android:layout_alignRight="@+id/txtProtection"
        android:text="@string/o"
        android:textSize="20sp" />

    <Button
        android:id="@+id/btnPrint"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnDialog"
        android:layout_alignBottom="@+id/btnDialog"
        android:layout_alignLeft="@+id/tv8"
        android:text="@string/Print"
        android:textSize="18sp" />

    <Button
        android:id="@+id/btnDialog"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/spinnerYear"
        android:layout_below="@+id/txtFinished"
        android:layout_marginTop="25dp"
        android:text="@string/Detail"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtCarAge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv5"
        android:layout_toLeftOf="@+id/viewMain"
        android:text="@string/o"
        android:textSize="20sp" />
</RelativeLayout>
